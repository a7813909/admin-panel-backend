{
      "name": "my-personnel-management-backend",
      "version": "1.0.0",
      "description": "Backend API for a personnel management admin panel, built with Node.js, Express, TypeScript, Prisma, and PostgreSQL.",
      "main": "dist/server.js",
      "scripts": {
            "build": "npx tsc",
            "start": "node dist/server.js",
            "dev": "ts-node-dev --respawn --transpile-only --files src/server.ts",
            "test": "echo \"No tests yet!\" && exit 0",
            "prisma:migrate": "npx prisma migrate dev --name",
            "prisma:generate": "npx prisma generate",
            "prisma:dbPull": "npx prisma db pull",
            "prisma:studio": "npx prisma studio"
      },
      "keywords": ["node", "typescript", "prisma", "admin-panel", "postgresql"],
      "author": "Alex_Yanikoff",
      "license": "ISC",
      "dependencies": {
            "@prisma/client": "^5.22.0",
            "bcryptjs": "^3.0.3",
            "cors": "^2.8.5",
            "dotenv": "^17.2.3",
            "express": "^4.19.2",
            "helmet": "^8.1.0",
            "jsonwebtoken": "^9.0.2",
            "prisma": "^5.12.1",
            "winston": "^3.18.3",
            "zod": "^4.1.12"
      },
      "devDependencies": {
            "@types/bcryptjs": "^2.4.6",
            "@types/cors": "^2.8.19",
            "@types/express": "^4.17.21",
            "@types/jsonwebtoken": "^9.0.10",
            "@types/node": "^20.12.7",
            "@types/winston": "^2.4.4",
            "ts-node-dev": "^2.0.0",
            "typescript": "^5.4.5"
      }
}